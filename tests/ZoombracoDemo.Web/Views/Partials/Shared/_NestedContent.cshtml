@using Zoombraco.Models
@model ZoombracoDemo.Logic.Models.INested

@{
    var orderedModel = Model.NestedContent.Where(c => c.DocumentTypeAlias.ToFirstUpperInvariant() == "NestedCheque").OrderByDescending(c => ((ZoombracoDemo.Logic.Models.NestedCheque)c).ChequeDate);
 }

@foreach (NestedComponent content in orderedModel)
{
    Html.RenderPartial(string.Format("~/Views/Partials/Shared/Nested/_{0}.cshtml", content.DocumentTypeAlias.ToFirstUpperInvariant()), content);
}
